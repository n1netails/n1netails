{
  "name": "typescript",
  "version": "0.2.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "spring-boot-build": "ng build --configuration=development && xcopy dist\\typescript\\browser\\ ..\\resources\\static\\* /E /I /Y",
    "spring-boot-build-production": "ng build --configuration=production && xcopy dist\\typescript\\browser\\ ..\\resources\\static\\* /E /I /Y",
    "spring-boot-build-unix": "ng build --configuration=development && cp -r dist/typescript/browser/* ../resources/static/",
    "spring-boot-build-unix-production": "ng build --configuration=production && cp -r dist/typescript/browser/* ../resources/static/"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.4",
    "@angular/compiler": "^20.0.4",
    "@angular/core": "^20.0.4",
    "@angular/forms": "^20.0.4",
    "@angular/platform-browser": "^20.0.4",
    "@angular/platform-browser-dynamic": "^20.0.4",
    "@angular/router": "^20.0.4",
    "@auth0/angular-jwt": "^5.2.0",
    "chart.js": "^4.4.9",
    "ng-zorro-antd": "^19.2.2",
    "ng2-charts": "^8.0.0",
    "ngx-markdown": "^19.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.4",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "less": "^4.2.0",
    "typescript": "~5.8.3"
  }
}